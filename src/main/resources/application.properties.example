
# Don't Create tables, just try and read from them
spring.jpa.hibernate.ddl-auto=none

# JDBC URL for the DataBase
spring.datasource.url=jdbc:mysql://<HOST_FOR_DATABASE>.us-east-1.rds.amazonaws.com:3306/<DATABASE_NAME>

# Username
spring.datasource.username=<USERNAME>

# Password
spring.datasource.password=<PASSWORD>